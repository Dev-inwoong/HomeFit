<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dreport_item_cl_result"
    android:layout_width="296dp"
    android:layout_height="80dp"
    android:layout_marginTop="16dp">

    <ImageView
        android:id="@+id/dreport_item_iv_temp_item_background"
        android:layout_width="296dp"
        android:layout_height="80dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/wohistory_iv_item"/>

    <TextView
        android:id="@+id/dreport_item_tv_result_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/eulyoo1945_regular"
        android:includeFontPadding="false"
        android:text=""
        android:textColor="@color/white"
        android:textSize="22sp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent">
        <TextView
            android:id="@+id/dreport_item_tv_result_calorie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_regular"
            android:includeFontPadding="false"
            android:text=""
            android:textColor="@color/lightGray"
            android:textSize="15sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/dreport_item_tv_result_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_regular"
            android:includeFontPadding="false"
            android:text=""
            android:textColor="@color/lightGray"
            android:layout_marginTop="8dp"
            android:textSize="15sp"
            app:layout_constraintTop_toBottomOf="@id/dreport_item_tv_result_calorie"
            app:layout_constraintEnd_toEndOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>